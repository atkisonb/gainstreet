<h1>Listing fund_managements</h1>

<table>
  <thead>
    <tr>
      <th>Relationship type</th>
      <th>Fund manager</th>
      <th>Fund</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fund_managements.each do |fund_management| %>
      <tr>
        <td><%= fund_management.relationship_type %></td>
        <td><%= fund_management.fund_manager %></td>
        <td><%= fund_management.fund %></td>
        <td><%= link_to 'Show', fund_management %></td>
        <td><%= link_to 'Edit', edit_fund_management_path(fund_management) %></td>
        <td><%= link_to 'Destroy', fund_management, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fund management', new_fund_management_path %>
